<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<context:annotation-config />

<!-- Definindo bean para profile -->
<bean id="profile"
class="com.tutorialspoint.springframework.annotation.qualifier.Profile">
</bean>

<!-- definindo bean para student1 -->
<bean id="student1"
class="com.tutorialspoint.springframework.annotation.qualifier.Student">

<property name="name" value="Zara" />
<property name="age" value="11" />
</bean>

<!--  defindo bean para student2 -->
<bean id="student2"
class="com.tutorialspoint.springframework.annotation.qualifier.Student">

<property name="name" value="Daniel Dias" />
<property name="age" value="25"/>
</bean>

</beans>
